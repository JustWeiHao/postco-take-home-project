{
    "include": ["env.d.ts", "**/*.ts", "**/*.tsx", "server.js"],
    "exclude": ["node-modules"],
    "compilerOptions": {
      "skipLibCheck": true,
      "lib": ["DOM", "DOM.Iterable", "ES2022"],
      "module": "ESNext",
      "isolatedModules": true,
      "esModuleInterop": true,
      "jsx": "react-jsx",
      "resolveJsonModule": true,
      "moduleResolution": "Bundler",
      "noEmit": true,
      "baseUrl": "./",
      "paths": {
        "~/*": ["./app/*"]
      },
    },
}
  